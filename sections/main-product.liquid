                                              </div>
                        <div class="product-bundle__price">
                       <span style="text-decoration:  line-through; color:#838382; font-size: 1.2rem;">  {{ variant.compare_at_price | money }}</span>
                          <span>{{ variant.price | money }}</span>
                         
                          <span>    {% if variant.price < variant.compare_at_price %}
                            {%- assign discount_value = variant.compare_at_price
                              | minus: variant.price
                              | times: 100.0
                              | divided_by: variant.compare_at_price
                              | round: 0
                              | append: '%'
                            -%}
                            <strong style="    color: #000000;   background: #84DADB;    padding: 3px 5px;    border-radius: 10px;     font-weight: 100;
    font-size: 1rem;">
                              Save {{ discount_value -}}
                            </strong>
                          {% endif %}</span>
                        </div>
                
                      </div>
                          <div>
                        {% if variant.image != blank %}
                          <img src="{{ variant.image | image_url: width: 50 }}" alt="{{ variant.title }}" class="variant-image" />
                        {% endif %}
                        </div>
                      {% if variant.metafields.custom.popular_ribbon == true %}
                        <div class="product-bundle__ribbon">
                          {{ block.settings.popular_text }}
                        </div>
                      {% endif %}

                      {%- if forloop.last -%}
                        <div class="product-bundle__ribbon">
                          {{ block.settings.most_value_text }}
                        </div>
                      {%- endif -%}
                    </label>
                  {%- endfor -%}

                  <script type="application/json">
                    {{ product.variants | json }}
                  </script>
                </dc-variant-bundles>
            {%- endcase -%}
          {%- endfor -%}

          <a href="{{ product.url }}" class="link product__view-details animate-arrow">
            {{ 'products.product.view_full_details' | t }}
            {% render 'icon-arrow' %}
          </a>
        </product-info>
      </div>
    </div>

    {% render 'product-media-modal', variant_images: variant_images %}

    {% comment %}
      ************************************************************
      SIZE GUIDES
      ************************************************************
    {% endcomment %}
    {% assign popups = section.blocks | where: 'type', 'popup' %}
    {% assign size_guides = section.blocks | where: 'type', 'size_guide' %}
    {%- assign popup_blocks = popups | concat: size_guides -%}
    {%- for block in popup_blocks -%}
      <modal-dialog id="PopupModal-{{ block.id }}" class="product-popup-modal" {{ block.shopify_attributes }}>
        <div
          role="dialog"
          aria-label="{{ block.settings.text }}"
          aria-modal="true"
          class="product-popup-modal__content"
          tabindex="-1"
        >
          <button
            id="ModalClose-{{ block.id }}"
            type="button"
            class="product-popup-modal__toggle"
            aria-label="{{ 'accessibility.close' | t }}"
          >
            {% render 'icon-close' %}
          </button>
          <div class="product-popup-modal__content-info rte">
            <div class="h2">{{ block.settings.page.title | escape }}</div>
            {{ block.settings.page.richtext }}
            {{ block.settings.page.textarea }}
            {{ block.settings.page.content }}
            {{ block.settings.page.liquid }}
          </div>
        </div>
      </modal-dialog>
    {%- endfor -%}

    {% comment %}
      ************************************************************
      STICKY CART
      ************************************************************
    {% endcomment %}
    {%- assign sticky_cart = section.blocks | where: 'type', 'sticky_cart' | first -%}

    {%- if sticky_cart != blank -%}
      <dc-sticky-cart class="product-sticky-cart" data-section="{{ section.id }}">
        <div class="product-sticky-cart__container page-width">
          <div class="product-sticky-cart__content">
            {%- if product.featured_image != blank -%}
              <div class="product-sticky-cart__media">
                {{ product.featured_image | image_url: width: 100 | image_tag: loading: 'lazy' }}
              </div>
            {%- endif -%}
            <div class="product-sticky-cart__info">
              <span class="product-sticky-cart__title">{{ product.title }}</span>
              <span class="product-sticky-cart__review">     13,950 Reviews</span>
              <span class="product-sticky-cart__price">
                <div id="StickyCartPrice-{{ section.id }}" role="status">
                  {%- render 'price',
                    product: product,
                    use_variant: true,
                    show_badges: false,
                    price_class: 'price--large'
                  -%}
                </div>
              </span>
            </div>
          </div>
          <div class="product-sticky-cart__addtocart">
            <button
              class="product-sticky-cart__addtocart-btn button button--primary"
              data-addtocart
            >
              <span>
                {%- if product.selected_or_first_available_variant.available == false -%}
                  {{ 'products.product.sold_out' | t }}
                {%- else -%}
                  {{ 'products.product.add_to_cart' | t }}
                {%- endif -%}
              </span>
              {%- render 'loading-spinner' -%}
            </button>
          </div>
        </div>
      </dc-sticky-cart>
    {%- endif -%}

    {%- if product.media.size > 0 -%}
      <script src="{{ 'product-modal.js' | asset_url }}" defer="defer"></script>
      <script src="{{ 'media-gallery.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}

    {%- if first_3d_model -%}
      <script type="application/json" id="ProductJSON-{{ product.id }}">
        {{ product.media | where: 'media_type', 'model' | json }}
      </script>
      <script src="{{ 'product-model.js' | asset_url }}" defer></script>
    {%- endif -%}

    {%- liquid
      if product.selected_or_first_available_variant.featured_media
        assign seo_media = product.selected_or_first_available_variant.featured_media
      else
        assign seo_media = product.featured_media
      endif
    -%}

    <script type="application/ld+json">
      {
        "@context": "http://schema.org/",
        "@type": "Product",
        "name": {{ product.title | json }},
        "url": {{ request.origin | append: product.url | json }},
        {% if seo_media -%}
          "image": [
            {{ seo_media | image_url: width: 1920 | prepend: "https:" | json }}
          ],
        {%- endif %}
        "description": {{ product.description | strip_html | json }},
        {% if product.selected_or_first_available_variant.sku != blank -%}
          "sku": {{ product.selected_or_first_available_variant.sku | json }},
        {%- endif %}
        "brand": {
          "@type": "Brand",
          "name": {{ product.vendor | json }}
        },
        "offers": [
          {%- for variant in product.variants -%}
            {
              "@type" : "Offer",
              {%- if variant.sku != blank -%}
                "sku": {{ variant.sku | json }},
              {%- endif -%}
              {%- if variant.barcode.size == 12 -%}
                "gtin12": {{ variant.barcode }},
              {%- endif -%}
              {%- if variant.barcode.size == 13 -%}
                "gtin13": {{ variant.barcode }},
              {%- endif -%}
              {%- if variant.barcode.size == 14 -%}
                "gtin14": {{ variant.barcode }},
              {%- endif -%}
              "availability" : "http://schema.org/{% if variant.available %}InStock{% else %}OutOfStock{% endif %}",
              "price" : {{ variant.price | divided_by: 100.00 | json }},
              "priceCurrency" : {{ cart.currency.iso_code | json }},
              "url" : {{ request.origin | append: variant.url | json }}
            }{% unless forloop.last %},{% endunless %}
          {%- endfor -%}
        ]
      }
    </script>
  </div>
</section>

